<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>GetARes</display-name>
  <welcome-file-list>
    <welcome-file>Index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>signUpServlet</servlet-name>
    <servlet-class>resServlets.SignUpServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>signUpServlet</servlet-name>
    <url-pattern>/signUp</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>logInServlet</servlet-name>
    <servlet-class>resServlets.LogInServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>logInServlet</servlet-name>
    <url-pattern>/logIn</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>afterSignup</servlet-name>
    <servlet-class>resServlets.AfterSignupServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>afterSignup</servlet-name>
    <url-pattern>/after</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>profileServlet</servlet-name>
    <servlet-class>resServlets.ProfileServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>profileServlet</servlet-name>
    <url-pattern>/profile</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>resumeServlet</servlet-name>
    <servlet-class>resServlets.ResumeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>resumeServlet</servlet-name>
    <url-pattern>/Resume</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>logoutServlet</servlet-name>
    <servlet-class>resServlets.LogoutServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>logoutServlet</servlet-name>
    <url-pattern>/Logout</url-pattern>
  </servlet-mapping>
</web-app>